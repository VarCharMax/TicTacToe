@model TicTacToe.Models.ResetPasswordEmailModel

@{
    ViewData["Title"] = "SendResetPassword";
}

@section Desktop {
    <h2>@Localizer["DesktopTitle"]</h2>
}
@section Mobile {
    <h2>@Localizer["MobileTitle"]</h2>
}

<h1>@Localizer["You have requested a password reset, an email has been sent to {0}, please click the link to continue.", Model.Email]</h1>